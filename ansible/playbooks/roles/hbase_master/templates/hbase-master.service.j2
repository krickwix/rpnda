# {{ansible_managed}}

[Unit]
Description=Hbase Master

[Service]
Type=simple
User=root
Group=root
Environment=JAVA_HOME=/opt/jdk{{ jdk_version_file }}
ExecStart=/opt/{{ hbase_file_base }}/bin/hbase master start
ExecStop=/opt/{{ hbase_file_base }}/bin/hbase master stop

TimeoutSec=300

[Install]
WantedBy=multi-user.target
