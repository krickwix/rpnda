# {{ansible_managed}}

[Unit]
Description=Hbase Region Server

[Service]
Type=simple
User=root
Group=root
Environment=JAVA_HOME=/opt/jdk{{ jdk_version_file }}
ExecStart=/opt/{{ hbase_file_base }}/bin/hbase regionserver start
ExecStop=/opt/{{ hbase_file_base }}/bin/hbase regionserver stop

TimeoutSec=300

[Install]
WantedBy=multi-user.target
